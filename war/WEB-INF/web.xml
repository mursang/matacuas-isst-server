<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>Es_upm_isst</servlet-name>
		<servlet-class>es.upm.isst.Es_upm_isstServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>newInfraccion</servlet-name>
		<servlet-class>es.upm.isst.newInfraccionServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Es_upm_isst</servlet-name>
		<url-pattern>/es_upm_isst</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>newInfraccion</servlet-name>
		<url-pattern>/newInfraccion</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
